gpgcheck = 0
baseurl = http://koji.chtc.wisc.edu/mnt/koji/repos/osg-25-main-%(dver)s-prerelease/latest/%(basearch)s/
name = osg-25-main-%(dver)s-prerelease (%(basearch)s)
